{
  "name": "mtml",
  "displayName": "MTML",
  "description": "Syntax support for the Movable Type Markup Language.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "publisher": "Taku Amano",
  "license": "MIT",
  "homepage": "https://github.com/usualoma/vscode-mtml/blob/main/README.md",
  "repository": {
      "type": "git",
      "url": "https://github.com/usualoma/vscode-mtml.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "mtml",
        "aliases": [
          "MTML"
        ],
        "extensions": [
          "mtml"
        ],
        "configuration": "./mtml.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mtml",
        "scopeName": "source.mtml",
        "path": "./syntaxes/mtml.tmGrammar.json"
      },
      {
        "injectTo": [
          "source.mtml"
        ],
        "scopeName": "source.mtml.embedded",
        "path": "./syntaxes/mtml.embedded.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.1",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "tslint": "^6.1.3",
    "typescript": "^4.5.4",
    "vscode": "^1.1.37"
  }
}
